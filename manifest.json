{
  "manifest_version": 2,
  "name": "evga.kr stock alert",
  "description": "evga.kr stock alert",
  "version": "1.1",
  "browser_action": {
    "default_icon": {
      "16": "logo.png"
    },
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery_1.10.2.min.js"
    ]
  }
}